# Server Configuration
PORT=4000
NODE_ENV=development

# PostgreSQL Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=credit_jambo_client
DB_USER=your_db_username
DB_PASSWORD=your_db_password
